# Staging Environment Variables Template
# Copy this file to .env.staging and fill in the actual values

NODE_ENV=staging
PORT=3001
FRONTEND_URL=https://babysteps-frontend-git-develop.vercel.app

# Supabase Configuration
SUPABASE_URL=https://your-staging-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_staging_service_role_key_here

# OpenAI Configuration
OPENAI_API_KEY=your_staging_openai_api_key_here

# Email Configuration
RESEND_API_KEY=your_staging_resend_api_key_here
FROM_EMAIL=staging@yourdomain.com

# Security
JWT_SECRET=your_staging_jwt_secret_here
ENCRYPTION_KEY=your_staging_encryption_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=200
AI_RATE_LIMIT_MAX_REQUESTS=50