{
  "name": "isee-ai-tutor-backend",
  "version": "1.0.0",
  "description": "Backend API for ISEE AI Tutor",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
