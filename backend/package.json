    "db:migrate": "supabase db push",
    "db:seed": "tsx src/scripts/seed.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openai": "^4.20.1",
    "rate-limiter-flexible": "^2.4.2",
    "resend": "^2.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.0",
    "@types/supertest": "^2.0.16",
    "@vitest/coverage-v8": "^0.34.6",
    "supertest": "^6.3.3",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
